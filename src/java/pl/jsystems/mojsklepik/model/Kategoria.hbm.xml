<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.org/dtd//hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="pl.jsystems.mojsklepik.model.Kategoria" table="kategoria">
        <id name="idKategorii" type="long" column="id_kategorii">
            <generator class="sequence">
                <param name = "sequence">kategorie_seq</param>
            </generator>
        </id>
        <property name="nazwa" type="string" column="nazwa_kategorii"/>
        
        
    </class>
       </hibernate-mapping>